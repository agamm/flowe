{
  "name": "flowe",
  "version": "0.2.13",
  "private": false,
  "type": "module",
  "workspaces": [
    "flowe-cli",
    "flowe-sdk"
  ],
  "scripts": {
    "build": "node scripts/build.js",
    "test:cli": "npm run test --workspace=flowe-cli",
    "test:sdk": "npm run test --workspace=flowe-sdk",
    "test:watch:cli": "npm run test:watch --workspace=flowe-cli",
    "test:watch:sdk": "npm run test:watch --workspace=flowe-sdk",
    "e2e": "playwright test",
    "e2e:ui": "playwright test --ui",
    "lint": "npm run lint --workspace=flowe",
    "pack-test": "node scripts/pack-test.js",
    "prepublishOnly": "npm run build"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "files": [
    "dist/**/*"
  ],
  "main": "./dist/sdk.js",
  "types": "./dist/sdk.d.ts",
  "exports": {
    ".": {
      "types": "./dist/sdk.d.ts",
      "import": "./dist/sdk.js"
    }
  },
  "bin": {
    "flowe": "dist/cli.js"
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@radix-ui/react-dialog": "^1.1.4",
    "@radix-ui/react-label": "^2.1.1",
    "@radix-ui/react-separator": "^1.1.1",
    "@radix-ui/react-slot": "^1.1.2",
    "@radix-ui/react-switch": "^1.1.3",
    "@radix-ui/react-tooltip": "^1.1.6",
    "@xyflow/react": "^12.3.6",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "commander": "^11.1.0",
    "keyv": "^5.2.3",
    "lucide-react": "^0.471.0",
    "next": "15.2.3",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-resizable-panels": "^2.1.7",
    "tailwind-merge": "^2.6.0",
    "tailwindcss-animate": "^1.0.7"
  },
  "devDependencies": {
    "@playwright/test": "^1.40.1",
    "@types/node": "^22.13.11",
    "rimraf": "^5.0.5"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/agam/flowe.git"
  },
  "keywords": [
    "workflow",
    "sdk",
    "debug",
    "visualization",
    "graph",
    "cli"
  ],
  "license": "ISC",
  "description": "",
  "author": "",
  "bugs": {
    "url": "https://github.com/agam/flowe/issues"
  },
  "homepage": "https://github.com/agam/flowe#readme"
}
