{
	"name": "flowe-cli",
	"version": "0.2.1",
	"private": false,
	"type": "module",
	"description": "Flowe - A developer tool for visualizing and debugging program flows",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/agam/flowe.git",
		"directory": "flowe"
	},
	"keywords": [
		"cli",
		"workflow",
		"debug",
		"visualization",
		"graph"
	],
	"scripts": {
		"dev": "next dev --port 27182",
